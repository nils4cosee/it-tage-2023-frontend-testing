{
  "name": "frontend-testing-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings=0 --ext js,ts,vue",
    "test:vitest": "vitest --ui",
    "test:preview": "vitest-preview",
    "test": "run-p test:vitest test:preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@vue/test-utils": "^2.4.2",
    "ramda": "^0.29.1",
    "vue": "^3.3.8",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/user-event": "^14.5.1",
    "@types/ramda": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "cross-fetch": "4.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "msw": "2.0.11",
    "nanoid": "^5.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vitest-preview": "^0.0.1",
    "vue-tsc": "^1.8.22"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --max-warnings=0 --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
